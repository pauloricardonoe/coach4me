\components\\CoachItem\\index.tsx",["54"],"import React from 'react';\r\n\r\nimport api from '../../services/api';\r\n\r\nimport whatsappIcon from '../../assets/images/icons/whatsapp.svg';\r\n\r\nimport './styles.css';\r\n\r\nexport interface Coach {\r\n  id: number;\r\n  avatar: string;\r\n  bio: string;\r\n  cost: number;\r\n  name: string;\r\n  subject: string;\r\n  whatsapp: string;\r\n  class_schedule: [{\r\n    from: number;\r\n    to: number;\r\n    week_day: number;\r\n  }]\r\n}\r\n\r\ninterface CoachItemProps {\r\n  coach: Coach;\r\n}\r\n\r\nconst CoachItem: React.FC<CoachItemProps> = ({ coach }) => {\r\n  function createNewConnection() {\r\n    api.post('connections', {\r\n      coach_id: coach.id,\r\n    });\r\n  }\r\n\r\n  function minuteToHour(minutos: number) {\r\n    let hora = Math.trunc(minutos / 60);\r\n    let minuto = (hora - (minutos / 60)) * 60;\r\n\r\n    return hora.toLocaleString('pt-BR', { minimumIntegerDigits: 2 }) + ':' + minuto.toLocaleString('pt-BR', { minimumIntegerDigits: 2 });\r\n  }\r\n\r\n  function dayOfWeek(dia: number) {\r\n    let Dias = [\r\n      'Domingo',\r\n      'Segunda-feira',\r\n      'Terça-feira',\r\n      'Quarta-feira',\r\n      'Quinta-feira',\r\n      'Sexta-feira',\r\n      'Sábado'\r\n    ];\r\n\r\n    return Dias[dia];\r\n  }\r\n\r\n\r\n  return (\r\n    <article className=\"coach-item\">\r\n      <header>\r\n        <img src={coach.avatar} alt={coach.name} />\r\n        <div>\r\n          <strong>{coach.name}</strong>\r\n          <span>{coach.subject}</span>\r\n        </div>\r\n      </header>\r\n\r\n      <p>{coach.bio}</p>\r\n\r\n      <div id=\"divhorarios\">\r\n        <br/>\r\n        <strong>Horários</strong>\r\n\r\n        {coach.class_schedule.map((horario) => {\r\n          return <p>{minuteToHour(horario.from)} - {minuteToHour(horario.to)} - {dayOfWeek(horario.week_day)}</p>;\r\n        })}\r\n      </div>\r\n\r\n      <footer>\r\n        <p>Preço/Hora\r\n          <strong>R$ {coach.cost}</strong>\r\n        </p>\r\n        \r\n        <a\r\n          target=\"_blank\" \r\n          onClick={createNewConnection} \r\n          href={`https://wa.me/${coach.whatsapp}`}\r\n        >\r\n          <img src={whatsappIcon} alt=\"Whatsapp\"/>\r\n          Entrar em contato\r\n        </a>\r\n      </footer>\r\n    </article>\r\n  );\r\n}\r\n\r\nexport default CoachItem;\r\n",{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line":83,"column":9,"nodeType":"61","endLine":87,"endColumn":10},"no-native-reassign",["62"],"no-negated-in-lhs",["63"],"react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" is a security risk: see https://html.spec.whatwg.org/multipage/links.html#link-type-noopener","JSXOpeningElement","no-global-assign","no-unsafe-negation"]